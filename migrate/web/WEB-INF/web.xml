<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

  <servlet>
  		<servlet-name>planManager</servlet-name>
  		<servlet-class>com.hongyuan.migrate.servlet.PlanManager</servlet-class>
  </servlet>
  
  <servlet-mapping>
  		<servlet-name>planManager</servlet-name>
  		<url-pattern>/manager/planManager.action</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>/jsp/login.jsp</welcome-file>
  </welcome-file-list>
</web-app>
